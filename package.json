{
  "name": "nuxt-spa-bootstrap5",
  "description": "Nuxt JS SPA & Bootstrap CSS Boilerplate Setup Application (JavaScript base)",
  "version": "1.0.0",
  "private": true,
  "email" : "asapdotid@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/asapdotid/nuxt-basic-spa-v2-bootstrap"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf .nuxt && rimraf dist",
    "dev": "nuxt --dotenv .env.development",
    "build:dev": "yarn clean && nuxt build --dotenv .env.development",
    "start:dev": "nuxt start --dotenv .env.development",
    "generate:dev": "nuxt generate --dotenv .env.development",
    "build:staging": "yarn clean && nuxt build --dotenv .env.staging",
    "start:staging": "pm2 start ecosystem.config.js --only staging-app && pm2 logs",
    "reload:staging": "pm2 reload ecosystem.config.js --only staging-app",
    "stop:staging": "pm2 stop ecosystem.config.js --only staging-app",
    "delete:staging": "yarn stop:staging && pm2 delete ecosystem.config.js --only staging-app",
    "build:production": "yarn clean && nuxt build --dotenv .env.production",
    "start:production": "pm2 start ecosystem.config.js --only production-app && pm2 logs",
    "reload:production": "pm2 reload ecosystem.config.js --only production-app",
    "stop:production": "pm2 stop ecosystem.config.js --only production-app",
    "delete:production": "yarn stop:production && pm2 delete ecosystem.config.js --only production-app"
  },
  "dependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@dansmaculotte/nuxt-security": "^0.0.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/style-resources": "^1.2.0",
    "@popperjs/core": "^2.9.2",
    "babel-core": "7.0.0-bridge.0",
    "bootstrap": "^5.0.2",
    "core-js": "^3.9.1",
    "fibers": "^5.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "lqip-loader": "^2.2.1",
    "nuxt": "^2.15.3",
    "nuxt-start": "^2.15.7",
    "nuxt-webfontloader": "^1.1.0",
    "responsive-loader": "^2.3.0",
    "rimraf": "^3.0.2",
    "sass": "^1.35.1",
    "sass-loader": "10",
    "sharp": "^0.28.3",
    "sqip-loader": "^1.0.0",
    "webp-loader": "^0.6.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.1.3",
    "babel-jest": "^26.6.3",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "vue-jest": "^3.0.4"
  }
}
